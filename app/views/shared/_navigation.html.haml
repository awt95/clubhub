%nav
  %div#navigation
    %ul
      - @items = [{:name=>'Home', :link=>'/'}, {:name=>'Find Clubs', :link=>'/clubs'}, {:name=>'Your Clubs', :link=>'/your_clubs'}]
      - @items.each do |item|
        %li= link_to item[:name], item[:link]
      - if user_signed_in?
        %li= link_to('Logout', destroy_user_session_path, method: :delete)
      - else
        %li= link_to('Login', new_user_session_path)